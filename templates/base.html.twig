												<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
									        <!--[endif]!-->
		{% endblock %}

		{% block javascripts %}
			<script src="{{asset('assets/js/jquery.js')}}"></script>

			<!--modernizr.min.js-->
