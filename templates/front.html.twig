<script>
    $(document).ready(function() {
        // Initialize the controlling bar
        $("#priceRange").ionRangeSlider({
            type: "double",
            min: 0,
            max: 100,
            from: 0,
            to: 100,
            postfix: "$",
            onFinish: function(data) {
                // Update the form input values when the range changes
                $("#minPrice").val(data.from);
                $("#maxPrice").val(data.to);

                // Optionally, you can submit the form or trigger the search here
                // $("#yourSearchForm").submit();
            }
        });
    });
</script>

